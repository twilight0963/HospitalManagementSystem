<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.hms.web.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>PatientServlet</servlet-name>
        <servlet-class>com.hms.web.PatientServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PatientServlet</servlet-name>
        <url-pattern>/patientServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>WardServlet</servlet-name>
        <servlet-class>com.hms.web.WardServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WardServlet</servlet-name>
        <url-pattern>/wardServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AppointmentServlet</servlet-name>
        <servlet-class>com.hms.web.AppointmentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AppointmentServlet</servlet-name>
        <url-pattern>/appointmentServlet</url-pattern>
    </servlet-mapping>
</web-app>